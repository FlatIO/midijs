{
    "rules": {
        "radix": 2,
        "vars-on-top": 2,
        "wrap-iife": 2,
        "valid-jsdoc": [2, {
            "prefer": {
                "returns": "return",
                "throws": "exception",
                "class": "constructor",
                "augments": "extends"
            }
        }],
        
        "brace-style": 2,
        "comma-style": [2, "last"],
        "quotes": [2, "simple"],
        "no-trailing-spaces": 0
    },
    
    "env": {
        "browser": true,
        "node": true
    }
}